    "@nestjs/typeorm": "^8.0.2",
    "@sendgrid/client": "^7.6.0",
    "@sendgrid/mail": "^7.6.0",
    "@types/clamscan": "^2.0.0",
    "@types/redis": "^2.8.32",
    "@types/stream-to-array": "^2.3.0",
    "@types/uuid": "^8.3.3",
    "ansi-styles": "^6.1.0",
    "aws-sdk": "^2.1046.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "bull": "^3.29.3",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
    "clamscan": "^2.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "eslint-config-airbnb": "^19.0.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
